<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mzl.alteruserimage.dao.UserDao">

    <!--保存用户信息到数据库-->
    <update id="saveUser">
        update user set image_path = #{imagePath} where username = #{username}
    </update>


</mapper>